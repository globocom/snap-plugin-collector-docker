{
  "version": 1,
  "schedule": {
    "type": "simple",
    "interval": "5s"
  },
  "workflow": {
    "collect": {
      "metrics": {
        "/intel/docker/*/*/*/spec/*": {},
        "/intel/docker/*/*/*/stats/cgroups/cpu_stats/*": {},
        "/intel/docker/*/*/*/stats/cgroups/memory_stats/*": {}
      },
      "config": {},
      "publish": [
        {
          "plugin_name": "file",
          "config": {
            "file": "/tmp/snap-docker-file.log"
          }
        }
      ]
    }
  }
}
